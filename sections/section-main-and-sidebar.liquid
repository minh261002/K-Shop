              {
                "value": "style1",
                "label": "Style 1"
              },
              {
                "value": "style2",
                "label": "Style 2"
              }

            ],
            "label": "Select style",
            "default": "style1"
        },
        {
          "type": "image_picker",
          "id": "image_slider2",
          "label": "Image"
        },
        
        {
          "type": "text",
          "id": "slider2_text_1",
          "label": "Text 1",
          "default":"<span>mo</span>dell"
        },
        {
          "type": "color",
          "id": "slider2_text_1_color",
          "label": "Text color"
        },
        
        {
          "type": "text",
          "id": "slider2_text_2",
          "label": "Text 2",
          "default":"for Women <span>2016</span>"
        },
        {
          "type": "color",
          "id": "slider2_text_2_color",
          "label": "Text color"
        },
        
        {
          "type": "text",
          "id": "slider2_text_3",
          "label": "Text 3",
          "default":"All product for girl",
          "info":"Use for style 2"
        },
        {
          "type": "color",
          "id": "slider2_text_3_color",
          "label": "Text color"
        },
        {
          "type": "text",
          "id": "slider2_text_button",
          "label": "Text button",
          "default":"Shop now!"
        },
        {
          "type": "url",
          "id": "slider2_text_button_url",
          "label": "Url"
        },
        {
          "type": "color",
          "id": "slider2_text_button_color",
          "label": "Text color"
        },
        {
          "type": "color",
          "id": "slider2_button_background",
          "label": "bachground button"
        },
        {
          "type": "header",
          "content": "Slider 3"
        },
        {
            "type": "select",
            "id": "style_slider3",
            "options": [
              {
                "value": "style1",
                "label": "Style 1"
              },
              {
                "value": "style2",
                "label": "Style 2"
              }

            ],
            "label": "Select style",
            "default": "style1"
        },
