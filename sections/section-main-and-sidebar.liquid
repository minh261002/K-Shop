
<div data-section-id="{{ section.id }}" data-section-type="section-main-content-and-sidebar" style="{% if section.settings.margin_top != blank %}margin-top: {{section.settings.margin_top}}px;{%endif%} {% if section.settings.margin_bottom != blank %}margin-bottom: {{section.settings.margin_bottom}}px;{% endif %}">
  <section class="main-and-sidebar">
    <div class="container">
   <div class="main-content-home3">
                <div class="row">
                    <div class="col-md-9 col-sm-8 col-xs-12">
                      
                        <div class="content-left3">
                        {%for block in section.blocks%}
                          {%if block.type == 'slider' %}
                            <div class="banner-slider banner-slider3">
                                <div class="wrap-item" data-transition="fade" data-autoplay="true" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1]]">
                                    
                                  {%for i in (1..3)%}
                                  {%capture style %}style_slider{{i}}{%endcapture%}
                                  {%capture image %}image_slider{{i}}{%endcapture%}
                                  {%capture text1 %}slider{{i}}_text_1{%endcapture%}
                                  {%capture color1 %}slider{{i}}_text_1_color{%endcapture%}
                                  {%capture text2 %}slider{{i}}_text_2{%endcapture%}
                                  {%capture color2 %}slider{{i}}_text_2_color{%endcapture%}
                                  {%capture text3 %}slider{{i}}_text_3{%endcapture%}
                                  {%capture color3 %}slider{{i}}_text_3_color{%endcapture%}
                                  {%capture button %}slider{{i}}_text_button{%endcapture%}
                                  {%capture url %}slider{{i}}_text_button_url{%endcapture%}
                                  {%capture button_color %}slider{{i}}_text_button_color{%endcapture%}
                                  {%capture button_background %}slider{{i}}_button_background{%endcapture%}
                                  <div class="item-banner">
                                        <div class="banner-thumb">
                                            <a href="{{block.settings[url]}}">
                                              
                                           {%if block.settings[image] == blank%}
                                            <img  src="{{'slider_home3.png'| asset_url }}" alt="">
                                            {%else%}
                                            <img src="{{block.settings[image]|img_url:'master'}}" alt="" />
                                            {%endif%}

                                          </a>
                                        </div>
                                    {%if block.settings[style] =="style1"%}
                                        <div class="banner-info white animated" data-animated="zoomInLeft">
                                            
                                          <h2 class="title60" style="color:{{block.settings[color1]}}">{{block.settings[text1]}}</h2>
                                          <h2 class="title30" style="color:{{block.settings[color2]}}">{{block.settings[text2]}}</h2>
                                          
                                          <a href="{{block.settings[url]}}" class="shopnow" style="color:{{block.settings[button_color]}}; background:{{block.settings[button_background]}};"><span>{{block.settings[button]}}</span></a>

                                        </div>
                                    {%else%}
                                    <div class="banner-info style2 animated" data-animated="zoomInRight">
                                            <h3 class="title30"  style="color:{{block.settings[color1]}}">{{block.settings[text1]}}</h3>
                                            <h2 class="title60"  style="color:{{block.settings[color2]}}">{{block.settings[text2]}}</h2>
                                            <h4 class="title18" style="color:{{block.settings[color3]}}">{{block.settings[text3]}}</h4>
                                             <a href="{{block.settings[url]}}" class="shopnow" style="color:{{block.settings[button_color]}}; background:{{block.settings[button_background]}};"><span>{{block.settings[button]}}</span></a>
                                            </div>
                                    {%endif%}
                                    </div>
                                  {%endfor%}
                                  
                                </div>
                            </div>
                          {%endif%}

                            <!-- End Banner Slider -->
                       
                          {%if block.type == 'coupon' %}
                            <div class="top-coupon">
                                <h2 class="title14 white">{{block.settings.coupon_title}}</h2>
                                <div class="coupon-slider arrow-style3">
                                  
                                    <div class="wrap-item" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1],[480,2],[1024,3]]">
                                      {%for i in (1..8) %}
                                      {%capture enable%}coupon{{i}}_enable{%endcapture%}
                                      {%capture image%}coupon{{i}}_image{%endcapture%}
                                      {%capture text%}coupon{{i}}_text{%endcapture%}
                                      {%capture color%}coupon{{i}}_button_color{%endcapture%}
                                      {%capture button%}coupon{{i}}_button{%endcapture%}
                                      {%capture url%}coupon{{i}}_url{%endcapture%}
                                      {%if block.settings[enable]%}
                                        <div class="item-coupon">
                                            <a href="{{block.settings[url]}}" class="logo-coupon">
                                              
                                              {%if block.settings[image] == blank %}
                                              <img class="pulse-shrink" alt="" src="{{'br1.png'|asset_url}}">
                                              {%else%}
                                              <img class="pulse-shrink" alt="" src="{{block.settings[image]|img_url:'medium'}}">
                                              {%endif%}
                                          </a>
                                            <p>{{block.settings[text]}} </p>
                                            <a href="{{block.settings[url]}}" class="btn-coupon wobble-horizontal" style="background:{{block.settings[color]}}">{{block.settings[button]}}</a>
                                        </div>
                                      {%endif%}
                                      {%endfor%}
                                    </div>
                                </div>
                            </div>
                          {%endif%}
                       
                            <!-- End Coupon -->
                           
                            {%if block.type == 'new-arrival' %}
                            <div class="new-product3">
                                <h2 class="title14 bg-color white">{{block.settings.arrival_title}}</h2>
                                <div class="newpro-slider3 arrow-style3">
                                    <div class="wrap-item" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1],[480,2],[1024,3]]">
                                      {%assign count = 1|plus:0 %}
                                      {%assign num = 0|plus:0 %}
                                {%for product in collections[block.settings.select_collection].products limit: block.settings.product_limit %}
                                    {%assign num = num|plus:1 %}
                                {%endfor%}
                                      {%for product in collections[block.settings.select_collection].products limit: block.settings.product_limit %}
                                      {%assign t = count|modulo:2 %}
                                    <!-- dk mo the div -->
                                      {%if count == 1 or opendiv == count %}
                                        <div class="item-newpro3">
                                         {%endif%}
                                          
                                            {%include 'product-item-v6'%}
                
                                    {%if t == 0 or count == num %}
                                    </div>
                                    {%endif%}
                                      {%if t == 0 %}
                                      {%assign opendiv = count|plus:1 %}
                                      {%endif%}
                                      {%assign count = count|plus:1 %}
                                  {%else%}
                                    {% include 'product-item-v6-empty' %}
                                      {%endfor%}
                                        <!-- End Item -->
                                
                                    </div>
                                </div>
                            </div>
                          {%endif%}
                    
                            <!-- End New Product -->
                       
                            {%if block.type == 'category' %}
                            <div class="cat-pro3  {%if block.settings.category_banner == false %} hidden-banner {%endif%}">
                                <h2 class="title14 title-catpro3">{{block.settings.category_title}}</h2>
                                <div class="catbn-slider3">
                                    <div class="wrap-item" data-pagination="false" data-navigation="false" data-itemscustom="[[0,1]]">
                                        <div class="banner-zoom">
                                            <a href="{{block.settings.category_url1}}" class="thumb-zoom">
                                              
                                              {%if block.settings.category_image1 == blank%}
                                                <img  src="{{'banner_category.png'| asset_url }}" alt="">
                                                {%else%}
                                                <img src="{{block.settings.category_image1|img_url:'grande'}}" alt="" />
                                                {%endif%}
                                          </a>
                                        </div>
                                      {%if block.settings.category_image2 != blank%}
                                        <div class="banner-zoom">
                                            <a href="{{block.settings.category_url2}}" class="thumb-zoom">
                                              
                                              {%if block.settings.category_image2 == blank%}
                                                <img  src="{{'banner_category.png'| asset_url }}" alt="">
                                                {%else%}
                                                <img src="{{block.settings.category_image2|img_url:'grande'}}" alt="" />
                                                {%endif%}
                                          </a>
                                        </div>
                                       {%endif%}
                                    </div>
                                </div>
                                <div class="catpro-slider3 arrow-style3">
                                    <div class="wrap-item" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1],[480,2],[980,3],[1200,4]]">
                                         {%for product in collections[block.settings.collection].products limit: block.settings.category_limit %}
                                            {%include 'product-item-v7'%}
                                      {%else%}
                                            {%include 'product-item-v7-empty'%}
                                      {%endfor%}
                                      
                                        
                                    </div>
                                </div>
                                <a href="#" class="btn-control-banner hide-cat-banner">hidden</a>
                                <a href="#" class="btn-control-banner show-cat-banner">show</a>
                            </div>
                          {%endif%}
                       {%endfor%}
                            <!-- End Cat Pro -->
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-4 col-xs-12">
                        <div class="content-right3">
                          {%for block in section.blocks%}
                          {%if block.type == 'sidebar-bestseller' %}
                            <div class="betpro3 box-side">
                                <h2 class="title14 white bg-color title-side">{{block.settings.bestseller_title}}</h2>
                                <div class="content-side toggle-tab toggle-betsale">
                                  {%for i in (1..5)%}
                                  {%capture enable%}colection_enable{{i}}{%endcapture%}
                                  {%capture title%}title_collection{{i}}{%endcapture%}
                                  {%capture collection%}select_collection{{i}}{%endcapture%}
                                  {%if block.settings[enable]%}
                                    <div class="item-toggle-tab {%if forloop.index == 1%}active{%endif%}">
                                        <h3 class="toggle-tab-title title14">{{block.settings[title]}} </h3>
                                        <div class="toggle-tab-content">
                                            <div class="row">
                                              
                                                    {%for product in collections[block.settings[collection]].products limit: block.settings.product_limit %}
                                      
                                                  <div class="col-md-6 col-sm-6 col-xs-6">
                                                
                                                    {%include 'sidebar-product-item-v8'%}

                                                    </div>

                                            {%else%}
                                              {% include 'sidebar-product-item-v8-empty' %}
                                                {%endfor%}
                                                </div>
                                                
                                            </div>
                                        </div>
                             
                                  {%endif%}
                                  {%endfor%}
                                    
                                </div>
                            </div>
                          {%endif%}
                            <!-- End Bet Pro -->
                           {%if block.type == 'sidebar-top-review' %}
                            <div class="box-side top-review3">
                                <h2 class="title14 white bg-color title-side">{{block.settings.review_title}}</h2>
                                <div class="content-side">
                                    <div class="review-slider3 arrow-style3">
                                        <div class="wrap-item" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1]]">
                                          {%assign count = 1|plus:0 %}
                                            {%assign num = 0|plus:0 %}
                                      {%for product in collections[block.settings.select_collection].products limit: block.settings.product_limit %}
                                          {%assign num = num|plus:1 %}
                                      {%endfor%}
                                      {%for product in collections[block.settings.select_collection].products limit: block.settings.product_limit %}
                                      {%assign t = count|modulo:3 %}
                                            
                                          <!-- dk mo the div -->
                                      {%if count == 1 or opendiv == count %}
                                        <div class="list-review3">
                                         {%endif%}
                                          
                                            {%include 'sidebar-product-itemlist-v9'%}
                
                                    {%if t == 0 or count == num %}
                                    </div>
                                    {%endif%}
                                      {%if t == 0 %}
                                      {%assign opendiv = count|plus:1 %}
                                      {%endif%}
                                      {%assign count = count|plus:1 %}
                                  {%else%}
                                    {% include 'sidebar-product-itemlist-v9-empty' %}
                                      {%endfor%}
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                          {%endif%}
                            <!-- End Top Review -->
                          {%if block.type == 'sidebar-top-brand' %}
                            <div class="top-brand3 box-side">
                                <h2 class="title14 white bg-color title-side">{{block.settings.brand_title}}</h2>
                                <div class="content-side">
                                    <div class="brand-slider3 arrow-style3">
                                        <div class="wrap-item" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1],[480,2],[768,1]]">
                                           {%assign count = 1|plus:0 %}
                                            {%assign num = 12|plus:0 %}
                                          {%for i in (1..12)%}
                                          {%capture enable %}brand{{i}}_enable{%endcapture%}
                                          {%capture image %}brand{{i}}_image{%endcapture%}
                                          {%capture url %}brand{{i}}_url{%endcapture%}
                                          {%assign t = count|modulo:6 %}
                                            
                                          <!-- dk mo the div -->
                                      {%if count == 1 or opendiv == count %}
                                        <div class="list-brand3">
                                         {%endif%}
                                          {%if block.settings[enable]%}
                                                <div class="logo-brand">
                                                    <a class="pulse" href="{{block.settings[url]}}">
                                                      {%if block.settings[image] == blank%}
                                                      <img  src="{{'br1.png'| asset_url }}" alt="">
                                                      {%else%}
                                                      <img src="{{block.settings[image]|img_url:'medium'}}" alt="" />
                                                      {%endif%}
                                                      
                                                  </a>
                                                </div>
                                             {%endif%}   
                                         {%if t == 0 or count == num %}
                                        </div>
                                        {%endif%}
                                          {%if t == 0 %}
                                          {%assign opendiv = count|plus:1 %}
                                          {%endif%}
                                          {%assign count = count|plus:1 %}
                                    
                                          {%endfor%}
                                           
                                        </div>
                                    </div>
                                </div>
                            </div>
                          {%endif%}
                            <!-- End Top Brand -->
                          {%if block.type == 'sidebar-advertisement' %}
                            <div class="advert3 box-side">
                                <h2 class="title14 white bg-color title-side">{{block.settings.advertisement_title}}</h2>
                                <div class="content-side">
                                    <div class="advert-slider3 arrow-style3">
                                        <div class="wrap-item" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1],[480,2],[768,1]]">
                                            {%for i in(1..4)%}
                                          {%capture enable %}ads{{i}}_enable{%endcapture%}
                                          {%capture image %}ads{{i}}_image{%endcapture%}
                                          {%capture title %}ads{{i}}_title{%endcapture%}
                                          {%capture text %}ads{{i}}_text{%endcapture%}
                                          {%capture button %}ads{{i}}_button{%endcapture%}
                                          {%capture url %}ads{{i}}_url{%endcapture%}
                                          {%if block.settings[enable]%}
                                          <div class="item-advert3">
                                                <div class="banner-zoom">
                                                    <a class="thumb-zoom" href="{{block.settings[url]}}">
                                                      {%if block.settings[image] == blank%}
                                                      <img  src="{{'banner-sidebar.png'| asset_url }}" alt=""/>
                                                      {%else%}
                                                      <img src="{{block.settings[image]|img_url:'medium'}}" alt="" />
                                                      {%endif%}
                                                  </a>
                                                </div>
                                                <h2 class="title14">{{block.settings[title]}}</h2>
                                                <p>{{block.settings[text]}}</p>
                                                <a href="{{block.settings[url]}}" class="btn-rect radius white bg-color">{{block.settings[button]}}</a>
                                            </div>
                                          {%endif%}
                                          {%endfor%}
                                          
                                        </div>
                                    </div>
                                </div>
                            </div>
                           {%endif%}
                            <!-- End Advert -->
                          {%if block.type == 'sidebar-tags' %}
                            <div class="tags3 box-side">
                                <h2 class="title14 white bg-color title-side">{{block.settings.tags_title}}</h2>
                                <div class="content-side">
                                    <div class="list-tags3">
                                      {%for i in (1..16)%}
                                      {%capture name %}tag{{i}}_name{%endcapture%}
                                      {%capture url %}url{{i}}_url{%endcapture%}
                                      {%if block.settings[name] !=blank%}
                                        <a href="{{block.settings[url]}}">{{block.settings[name]}}</a>
                                      {%endif%}
                                      {%endfor%}
                                        
                                    </div>
                                </div>
                            </div>
                           {%endif%}
                            <!-- End Tags -->
                          {%if block.type == 'sidebar_testimonial' %}
                            <div class="testimo3 box-side">
                                <h2 class="title14 white bg-color title-side">{{block.settings.testimonial_title}}</h2>
                                <div class="content-side">
                                    <div class="arrow-style3">
                                      
                                        <div class="wrap-item" data-pagination="false" data-navigation="true" data-itemscustom="[[0,1]]">   
                                          {%for i in (1..4)%}
                                          {%capture enable %}enable_testimonial{{i}}{%endcapture%}
                                          	{%capture image %}testimonial{{i}}_image{%endcapture%}
                                      		
                                          	{%capture text %}testimonial{{i}}_text{%endcapture%}
                                      		{%capture name %}testimonial{{i}}_name{%endcapture%}
                                          {%if block.settings[enable]%}
                                            <div class="item-testimo3">
                                                <div class="testimo-icon"><a href="#">
                                                  {%if block.settings[image] == blank%}
                                                      <img  src="{{'banner-sidebar.png'| asset_url }}" alt=""/>
                                                      {%else%}
                                                      <img src="{{block.settings[image]|img_url:'medium'}}" alt="" />
                                                      {%endif%}
                                                  </a></div>
                                                <p class="desc">{{block.settings[text]}}</p>
                                                <h3 class="title14"><a href="#">{{block.settings[name]}}</a></h3>
                                            </div>
                                          {%endif%}
                                          {%endfor%}
                                           
                                        </div>
                                    </div>
                                </div>
                            </div>
                           {%endif%}
                            <!-- End Testimo -->
                          {%if block.type == 'sidebar_store' %}
                            <div class="outstore3 box-side">
                                <h2 class="title14 white bg-color title-side">{{block.settings.store_title}}</h2>
                                <div class="content-side">
                                    <div class="outstore-link">
                                        <a href="{{block.settings.store_url}}">
                                          {%if block.settings.store_image == blank%}
                                          <img class="pulse"  src="{{'banner-sidebar.png'| asset_url }}" alt=""/>
                                          {%else%}
                                          <img class="pulse" src="{{block.settings.store_image|img_url:'medium'}}" alt="" />
                                          {%endif%}</a>
                                    </div>
                                </div>
                            </div>
                           {%endif%}
                            <!-- End Our Store -->
                          {%if block.type == 'sidebar_why_choose_us' %}
                            <div class="whyus3 box-side">
                                <h2 class="title14 white bg-color title-side">{{block.settings.choose_title}}</h2>
                                <div class="content-side">
                                    <ul class="list-none list-why">
                                        <li>
                                            <h3 class="title14"><strong>1.</strong>{{block.settings.why1_title}}</h3>
                                            <p>{{block.settings.why1_text}}</p>
                                        </li>
                                        <li>
                                            <h3 class="title14"><strong>2.</strong>{{block.settings.why2_title}}</h3>
                                            <p>{{block.settings.why2_text}}</p>
                                        </li>
                                        <li>
                                            <h3 class="title14"><strong>3.</strong>{{block.settings.why3_title}}</h3>
                                            <p>{{block.settings.why3_text}}</p>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                           {%endif%}
                          {%endfor%}
                            <!-- Why Choise Us -->
                        </div>
                    </div>
                </div>
            </div>
    </div>
  </section>
</div>

{% schema %}

  {
    "name": "Main content and sidebar",
    "class" : "index-section",
    "settings": [
      {
        "type": "header",
        "content": "Section Space"
      },
      {
        "type": "text",
        "id": "margin_top",
        "label": "Margin Top",
        "info": "Defined in pixels. Do not add the 'px' unit."
      },
      {
        "type": "text",
        "id": "margin_bottom",
        "label": "Margin Bottom",
        "info": "Defined in pixels. Do not add the 'px' unit."
      }
    ],
    "blocks": [
    {
      "type": "slider",
      "name": "Slider",
      "limit":1,
      "settings": [
        {
          "type": "header",
          "content": "Slider 1"
        },
        {
            "type": "select",
            "id": "style_slider1",
            "options": [
              {
                "value": "style1",
                "label": "Style 1"
              },
              {
                "value": "style2",
                "label": "Style 2"
              }

            ],
            "label": "Select style",
            "default": "style1"
        },
        {
          "type": "image_picker",
          "id": "image_slider1",
          "label": "Image"
        },
        
        {
          "type": "text",
          "id": "slider1_text_1",
          "label": "Text 1",
          "default":"<span>mo</span>dell"
        },
        {
          "type": "color",
          "id": "slider1_text_1_color",
          "label": "Text color"
        },
        
        {
          "type": "text",
          "id": "slider1_text_2",
          "label": "Text 2",
          "default":"for Women <span>2016</span>"
        },
        {
          "type": "color",
          "id": "slider1_text_2_color",
          "label": "Text color"
        },
        
        {
          "type": "text",
          "id": "slider1_text_3",
          "label": "Text 3",
          "default":"All product for girl",
          "info":"Use for style 2"
        },
        {
          "type": "color",
          "id": "slider1_text_3_color",
          "label": "Text color"
        },
        
        {
          "type": "text",
          "id": "slider1_text_button",
          "label": "Text button",
          "default":"Shop now!"
        },
        {
          "type": "url",
          "id": "slider1_text_button_url",
          "label": "Url"
        },
        {
          "type": "color",
          "id": "slider1_text_button_color",
          "label": "Text color"
        },
        {
          "type": "color",
          "id": "slider1_button_background",
          "label": "bachground button"
        },
        {
          "type": "header",
          "content": "Slider 2"
        },
        {
            "type": "select",
            "id": "style_slider2",
